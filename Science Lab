#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d",&n);
    int a[n],i,sum;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int max=a[0];
    for(i=0;i<n;i++)
    {
        if(max<a[i])
        {
            max=a[i];
        }
    }
    int max1=a[i];
    for(i=0;i<n;i++)
    {
        if(a[i]!=max)
        {
        if(max1<a[i])
        {
            max1=a[i];
        }
        }
    }
        sum=max+max1;
        printf("%d",sum);
    return 0;
}
