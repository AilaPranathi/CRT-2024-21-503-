#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int i,n;
    scanf("%d",&n);
    int a[n],sum1=0,sum2=0;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        if(a[i]%2==0)
        {
          sum1=sum1+a[i];
        }
        else
        {
            sum2=sum2+a[i];
        }
    }
    printf("%d\n%d",sum2,sum1);
    return 0;
}
