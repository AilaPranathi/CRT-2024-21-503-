#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,result=1;
    scanf("%d",&n);
    int i;
    for(i=2;i<=n;i++)
    {
    result=result*i;
    }
    int count=0;
    while(result%10==0)
    {
        count++;
        result/=10;
    }
    printf("%d",count);
    return 0;
}
