#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,digit;
    scanf("%d",&n);
    int sum=0;
    while(n>0)
    {
         digit=n%10;
        if(digit%2!=0)
        {
            sum=sum+digit;
        }
        n/=10;
    }
    printf("%d",sum);
    return 0;
}
